{
	"name": "webpack-handlebars-bootstrap",
	"version": "1.0.0",
	"description": "A clean Webpack 4 + Bootstrap 4 + Handlebars frontend template.",
	"scripts": {
		"start": "npm run watch",
		"eslint:fix": "eslint --fix",
		"build": "webpack --config webpack-prod.config.js --colors --optimize-minimize",
		"watch": "webpack-dev-server --config webpack-dev.config.js --watch --colors -d"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/saurabh73/webpack-handlebars-bootstrap.git"
	},
	"author": "Saurabh Dutta",
	"license": "MIT",
	"eslintConfig": {
		"env": {
			"browser": true,
			"node": true
		},
		"parserOptions": {
			"ecmaVersion": 6,
			"sourceType": "module"
		},
		"rules": {
			"semi": 2
		}
	},
	"homepage": "https://github.com/saurabh73/webpack-handlebars-bootstrap#readme",
	"devDependencies": {
		"@babel/core": "^7.4.4",
		"@babel/preset-env": "^7.4.4",
		"babel-loader": "^8.0.5",
		"css-loader": "^2.1.1",
		"eslint": "^5.16.0",
		"eslint-loader": "^2.1.2",
		"extract-text-webpack-plugin": "^4.0.0-beta.0",
		"handlebars": "^4.1.2",
		"handlebars-loader": "^1.7.1",
		"html-inline-css-webpack-plugin": "^1.11.1",
		"html-webpack-plugin": "^3.2.0",
		"node-sass": "^4.12.0",
		"sass-loader": "^7.1.0",
		"script-ext-html-webpack-plugin": "^2.1.5",
		"style-loader": "^0.23.1",
		"webpack": "^4.30.0",
		"webpack-cli": "^3.3.1",
		"webpack-dev-server": "^3.3.1"
	},
	"dependencies": {
		"bourbon": "^5.1.0",
		"core-util-is": "^1.0.2",
		"date-fns": "^2.22.1",
		"lodash": "^4.17.21",
		"paper-css": "^0.4.1",
		"reset-css": "^5.0.1"
	}
}
